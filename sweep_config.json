{
    "name": "Testing Sweep",
    "method": "random",
    "metric": {
      "goal": "maximize",
      "name": "metrics/mAP50(B)"
    },
    "parameters": {
      "patience": {
        "distribution": "q_uniform",
        "max": 100,
        "min": 25,
        "q": 25
      },
      "cos_lr": {
        "values": [false, true]
      },
      "dropout": {
        "values": [0.0, 0.2, 0.4, 0.6, 0.8]
      },
      "seed": {
        "values": [42, 111]
      },
      "box": {
        "distribution": "uniform",
        "max": 10,
        "min": 0.5
      },
      "cls": {
        "distribution": "uniform",
        "max": 10,
        "min": 0.5
      },
      "dfl": {
        "distribution": "uniform",
        "max": 10,
        "min": 0.5
      }
    }
}